{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "name": "$return",
      "type": "http",
      "direction": "out"
    },
    {
      "name": "outputblob",
      "type": "blob",
      "path": "https://ocrprojectdata.blob.core.windows.net/photos/",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}
